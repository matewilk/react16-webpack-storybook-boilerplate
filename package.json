{
  "name": "Rewards.Journey.Prototype",
  "version": "1.0.0",
  "description": "Rewards Journey Prototype",
  "main": "server.js",
  "scripts": {
    "client": "webpack-dev-server --progress --colors",
    "server": "nodemon --inspect=5858 -L server.js",
    "debug": "npm run server & npm run client",
    "build": "webpack --config webpack.prod.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/jest/fileTransformer.js"
    }
  },
  "author": "Mateusz Wilk",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "connect-mongo": "^1.3.2",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "flexboxgrid": "^6.3.1",
    "history": "^4.7.2",
    "material-ui": "^0.19.2",
    "node-sass": "^4.5.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-flexbox-grid": "^1.1.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "request": "^2.81.0",
    "shortid": "^2.2.8",
    "webpack": "^3.5.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack-dev-server": "^2.8.1"
  }
}
